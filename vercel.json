  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/((?!newsletters/|data/|images/|assets/|.*\\.).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "middleware.ts": {
      "runtime": "@vercel/edge"
    }
  }
}
